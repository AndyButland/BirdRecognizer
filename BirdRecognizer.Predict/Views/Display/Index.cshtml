@model DisplayViewModel
@{
    ViewData["Title"] = "Display Image";
}

<div class="row">
    <div class="col-md-12">
        <h2>Image results</h2>
        
        <h4>Uploaded image</h4>
        <img src="@Model.ImageUrl" width="400" />

        <h4>Classification</h4>

        <dl class="row">
            <dt class="col-sm-3">Status: </dt>
            <dd class="col-sm-9">@Model.ClassificationStatus</dd>
        </dl>
    </div>
</div>
